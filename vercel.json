{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "web_card_app/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/web_card_app/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web_card_app/$1"
    }
  ]
}